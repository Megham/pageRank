{
  "name": "Check PageRank",
  "version": "1.0",
  "description": "check the page rank of your page on the fly.",
    
  "browser_action": {
    "default_icon": "icon.png",
	 "default_popup": "popup.html"
  },
 "manifest_version": 2,
 "permissions": [ "tabs", "http://*/*", "https://*/*" ],
 
 "content_scripts": [
    {
      "matches": [ "http://*/*", "https://*/*" ],
      "js": ["page_rank.js", "popup.js", "player.js"],
	     "css": []
    }
	]
}